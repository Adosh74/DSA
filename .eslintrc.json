{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto",
                "trailingComma": "es5"
            }
        ],
        "no-console": 1,
        "no-var": 2,
        "no-undef": 1
    }
}
